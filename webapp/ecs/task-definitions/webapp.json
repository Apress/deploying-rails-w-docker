{
    "family": "webapp",
    "containerDefinitions": [
        {
            "name": "webapp",
            "image": "pacuna/webapp:LAST_COMMIT",
            "cpu": 500,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PASSENGER_APP_ENV",
                    "value": "production"
                }
            ]
        }
    ]
}
